    "version": "0.2.0",
    "configurations": [
        {
            "name": "Stable-Diffusion T2I Fill ImageNet",
            "type": "python",
            "request": "launch",
            "program": "multigpu/fill_imagenet.py",
            "env": {"CUDA_VISIBLE_DEVICES":"0, 1",},
            "args": [
            "--plms",
            "--n_samples", "4",
            "--fill_imagenet",
            "--imagenet_path", "/root/data2/data/ImageNet",
            "--imagenet_out_path", "/root/data/data/ImageNet-LT",
            "--target_images", "1300",
            "--DDP",
            "--gpus", "2",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "Stable-Diffusion T2I MultiGPU",
            "type": "python",
            "request": "launch",
            "program": "multigpu/txt2img.py",
            "env": {"CUDA_VISIBLE_DEVICES":"0, 1",},
            "args": [
            "--prompt", "a photo of a magpie'",
            "--plms",
            "--n_samples", "2",
            "--n_iter", "2", 
            "--seed", "1234",
            "--DDP",
            "--gpus", "2",
            "--H", "256",
            "--W", "256"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Stable-Diffusion T2I Inference",
            "type": "python",
            "request": "launch",
            "program": "scripts/txt2img.py",
            "env": {"CUDA_VISIBLE_DEVICES":"0"},
            "args": ["--prompt", "a photo of a magpie",
            "--plms",
            "--n_samples", "1",
            "--n_iter", "1", 
            "--seed", "67567",
            "--H", "256",
            "--W", "256",
            "--C", "4"
            // "--from_file", "prompts.txt"
            // "--precision", "full"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Stable-Diffusion I2I Inference",
            "type": "python",
            "request": "launch",
            "program": "scripts/img2img.py",
            "args": ["--prompt", " ",
            "--init-img",
            "/root/data/data/CIFAR10_PNG/train/bird/5000.png",
            "--n_samples", "1",
            "--n_iter", "1",
            "--seed", "334234",
            ],

            "console": "integratedTerminal",
            "justMyCode": true
        }

    ]
}