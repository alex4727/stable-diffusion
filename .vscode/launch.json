    "version": "0.2.0",
    "configurations": [
        {
            "name": "Stable-Diffusion T2I MultiGPU",
            "type": "python",
            "request": "launch",
            "program": "multi_gpu.py",
            "env": {"CUDA_VISIBLE_DEVICES":"0, 1",},
            "args": [
            "--prompt", "magpie",
            "--plms",
            "--n_samples", "6",
            "--n_iter", "2", 
            "--seed", "1234",
            "--DDP",
            "--gpus", "2",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Stable-Diffusion T2I MultiGPU-single",
            "type": "python",
            "request": "launch",
            "program": "multi_gpu.py",
            "env": {"CUDA_VISIBLE_DEVICES":"0",},
            "args": [
            "--prompt", "magpie",
            "--plms",
            "--n_samples", "2",
            "--n_iter", "2", 
            "--seed", "1234",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },


        {
            "name": "Stable-Diffusion T2I Inference",
            "type": "python",
            "request": "launch",
            "program": "scripts/txt2img.py",
            "env": {"CUDA_VISIBLE_DEVICES":"0"},
            "args": ["--prompt", "magpie",
            "--plms",
            "--n_samples", "3",
            "--n_iter", "2", 
            "--seed", "1234",
            "--fixed_code",
            // "--from_file", "prompts.txt"
            // "--precision", "full"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Stable-Diffusion I2I Inference",
            "type": "python",
            "request": "launch",
            "program": "scripts/img2img.py",
            "args": ["--prompt", " ",
            "--init-img",
            "/root/data/data/CIFAR10_PNG/train/bird/5000.png",
            "--n_samples", "1",
            "--n_iter", "1",
            "--seed", "334234",
            ],

            "console": "integratedTerminal",
            "justMyCode": true
        }

    ]
}